{
  "name": "foodtruckfinder",
  "version": "1.0.0",
  "description": "Team:",
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" && webpack",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "lint": "functionhappy | snazzy",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "if-env": "^1.0.4",
    "mysql": "^2.15.0",
    "nighthawk": "^2.1.1",
    "node-mandrill": "^1.0.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "concurrently": "^3.5.0",
    "functionhappy": "^1.0.0",
    "nodemon": "^1.11.0",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/max-liao/FoodTruckFinder"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/max-liao/FoodTruckFinder/issues"
  },
  "homepage": "https://github.com/max-liao/FoodTruckFinder#readme",
  "keywords": []
}
